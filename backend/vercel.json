{
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "crons": [
      {
      "path": "/utils/updateannouncements.py",
      "schedule": "0 */6 * * *"
    }
  ]
}